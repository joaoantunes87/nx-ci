{
  "name": "app-one",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/app-one/src",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project app-one --web",
  "targets": {
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "mv dist/apps/app-one apps/app-one/dist && cd apps/app-one && firebase deploy --project $FIREBASE_PROJECT_APP_ONE --only hosting --token $FIREBASE_TOKEN --non-interactive"
          }
        ]
      }
    },
    "deploy-azure": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "yarn swa deploy ./dist/apps/app-one -d 2df9529f57024f0155e386485b4728462d14f2350c716c7ddc1fdb03c1a00bc55-2ba5d840-7d14-4277-8907-84c836a81d6c010362489"
          }
        ]
      }
    }
  }
}
