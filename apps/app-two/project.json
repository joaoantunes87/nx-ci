{
  "name": "app-two",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/app-two/src",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project app-two --web",
  "targets": {
    "deploy-azure": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "yarn swa deploy ./dist/apps/app-two --env $AZURE_ENVIRONMENT -d $AZURE_TOKEN_APP_TWO"
          }
        ]
      }
    },
    "delete-azure-environment": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "az staticwebapp environment delete --environment-name $AZURE_ENVIRONMENT --token $AZURE_TOKEN_APP_TWO"
          }
        ]
      }
    }
  }
}
