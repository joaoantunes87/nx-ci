{
  "name": "app-two",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/app-two/src",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project app-two --web",
  "targets": {
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
